<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/mLinear_widthrawPPw"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="44dp">

        <ImageView
            android:id="@+id/Image_Back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16dp"
            android:src="@drawable/return_black_man" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="提现记录"
            android:textColor="@color/Black" />
    </RelativeLayout>

    <include layout="@layout/activity_user_backgroundline" />


    <RelativeLayout
        android:id="@+id/AliPay_Detail"
        android:layout_width="match_parent"
        android:layout_height="54dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:src="@drawable/more_gray_man"

            />

        <RelativeLayout
            android:id="@+id/alipay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="8dp">

            <TextView
                android:id="@+id/mText_AliPay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="支付宝" />

            <TextView
                android:id="@+id/mText_Time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="48dp"
                android:text="2017/10/11 15:57" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/alipay"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="8dp">

            <TextView
                android:id="@+id/mText_ChuLi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="处理中" />

            <TextView
                android:id="@+id/mText_Money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="48dp"
                android:text="1000" />


        </RelativeLayout>


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/BandCard_detail"
        android:layout_width="match_parent"
        android:layout_height="80dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:src="@drawable/more_gray_man" />

        <RelativeLayout
            android:id="@+id/BandCard"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="8dp">

            <TextView
                android:id="@+id/mText_BandCard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="银行卡" />

            <TextView
                android:id="@+id/mText_Time1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="48dp"
                android:text="2017/10/11 15:57" />


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relative_bandcard"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/BandCard"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="8dp">

            <TextView
                android:id="@+id/BandCard_mText_ChuLi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="未成功" />

            <TextView
                android:id="@+id/BandCard_mText_Money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="48dp"
                android:text="1000" />


        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relative_bandcard"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="10dp"
            android:drawableLeft="@drawable/tishitiao_man_4x"
            android:drawablePadding="12dp"
            android:text="账户卡号与账户名称不符，请核实后重新提交申请"
            android:textSize="12dp" />


    </RelativeLayout>


</LinearLayout>