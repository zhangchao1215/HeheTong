<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp">

        <LinearLayout

            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/UserSetting_SettingBirthDay"
                android:layout_width="72dp"
                android:layout_height="44dp"
                android:gravity="center"
                android:text="点击设置"
                android:textSize="12dp"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:drawablePadding="5dp"
                android:drawableTop="@drawable/shentixinxi34x"
                android:gravity="center"
                android:text="出生日期"

                />


        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/UserSetting_SettingSex"
                android:layout_width="72dp"
                android:layout_height="44dp"
                android:gravity="center"
                android:text="点击设置"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:drawablePadding="5dp"
                android:drawableTop="@drawable/shentixinxi34x"
                android:gravity="center"
                android:text="性别"

                />


        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/UserSetting_SettingShengao"
                android:layout_width="72dp"
                android:layout_height="44dp"
                android:gravity="center"
                android:text="点击设置"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:drawablePadding="5dp"
                android:drawableTop="@drawable/shentixinxi34x"
                android:gravity="center"
                android:text="身高"

                />


        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/UserSetting_SettingWeight"
                android:layout_width="72dp"
                android:layout_height="44dp"
                android:gravity="center"
                android:text="点击设置"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:drawablePadding="5dp"
                android:drawableTop="@drawable/shentixinxi34x"
                android:gravity="center"
                android:text="体重"

                />

        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp">

        <LinearLayout

            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/UserSetting_blood"
                android:layout_width="72dp"
                android:layout_height="44dp"
                android:gravity="center"
                android:src="@drawable/shentixinxi14x"
                android:text="点击设置"
                />

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:drawablePadding="5dp"
                android:drawableTop="@drawable/shentixinxi34x"
                android:gravity="center"
                android:text="血型"

                />


        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/UserSetting_SettingCity"
                android:layout_width="72dp"
                android:layout_height="44dp"
                android:gravity="center"
                android:text="点击设置"/>

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:drawablePadding="5dp"
                android:drawableTop="@drawable/shentixinxi34x"
                android:gravity="center"
                android:text="居住地"

                />


        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/UserSetting_SettingMarriage"
                android:layout_width="72dp"
                android:layout_height="44dp"
                android:gravity="center"
                android:text="点击设置"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:drawablePadding="5dp"
                android:drawableTop="@drawable/shentixinxi34x"
                android:gravity="center"
                android:text="婚姻状况"

                />


        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/UserSetting_SettingJop"
                android:layout_width="72dp"
                android:layout_height="44dp"
                android:gravity="center"
                android:maxLength="4"
                android:maxLines="1"
                android:src="@drawable/shentixinxi14x"
                android:text="点击设置"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:drawablePadding="5dp"
                android:drawableTop="@drawable/shentixinxi34x"
                android:gravity="center"
                android:text="当前职业"

                />

        </LinearLayout>


    </LinearLayout>

</LinearLayout>